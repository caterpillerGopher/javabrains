<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="nuroute" class="com.nuroute.NuRoute"
		
		scope="singleton" lazy-init="true">
	</bean>
	
	<bean id="triangle" class="com.one.Triangle">
	 <constructor-arg index = "0" value = "Equilateral"></constructor-arg>
	 <constructor-arg  index ="1" value = "10"></constructor-arg>
	<!-- <property name="type" value="Equilateral"></property> -->
	
	</bean>
	<bean id="circle" class="com.one.Circle">
	<property name="type" value="Small"></property>
	</bean>
	
</beans>
